<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <_NativePackages_Jolt_IsWindows Condition="'$([System.Runtime.InteropServices.RuntimeInformation]::IsOSPlatform($([System.Runtime.InteropServices.OSPlatform]::Windows)))' == 'true'">true</_NativePackages_Jolt_IsWindows>
    <_NativePackages_Jolt_IsMacOS Condition="'$([System.Runtime.InteropServices.RuntimeInformation]::IsOSPlatform($([System.Runtime.InteropServices.OSPlatform]::OSX)))' == 'true'">true</_NativePackages_Jolt_IsMacOS>
    <_NativePackages_Jolt_IsLinux Condition="'$([System.Runtime.InteropServices.RuntimeInformation]::IsOSPlatform($([System.Runtime.InteropServices.OSPlatform]::Linux)))' == 'true'">true</_NativePackages_Jolt_IsLinux>

    <_NativePackages_Jolt_NativeRuntime Condition=" '$(_NativePackages_Jolt_NativeRuntime)' == '' And '$(_NativePackages_Jolt_IsMacOS)' == 'true' And ('$(Prefer32Bit)' == 'false' Or '$(PlatformTarget)' == 'x64')">osx-x64</_NativePackages_Jolt_NativeRuntime>
    <_NativePackages_Jolt_NativeRuntime Condition=" '$(_NativePackages_Jolt_NativeRuntime)' == '' And '$(_NativePackages_Jolt_IsLinux)' == 'true' And ('$(Prefer32Bit)' == 'false' Or '$(PlatformTarget)' == 'x64')">linux-x64</_NativePackages_Jolt_NativeRuntime>
    <_NativePackages_Jolt_NativeRuntime Condition=" '$(_NativePackages_Jolt_NativeRuntime)' == '' And '$(_NativePackages_Jolt_IsWindows)' == 'true' And ('$(Prefer32Bit)' == 'true' Or '$(PlatformTarget)' == 'x86')">win-x86</_NativePackages_Jolt_NativeRuntime>
    <_NativePackages_Jolt_NativeRuntime Condition=" '$(_NativePackages_Jolt_NativeRuntime)' == '' And '$(_NativePackages_Jolt_IsWindows)' == 'true' And ('$(Prefer32Bit)' == 'false' Or '$(PlatformTarget)' == 'x64')">win-x64</_NativePackages_Jolt_NativeRuntime>
    <_NativePackages_Jolt_NativeRuntime Condition=" '$(_NativePackages_Jolt_NativeRuntime)' == '' And '$(_NativePackages_Jolt_IsWindows)' == 'true' And '$(PlatformTarget)' == 'arm64'">win-arm64</_NativePackages_Jolt_NativeRuntime>

    <_NativePackages_Jolt_NativeLibName Condition="'$(_NativePackages_Jolt_NativeRuntime)' == 'win-x86'">joltc.dll</_NativePackages_Jolt_NativeLibName>
    <_NativePackages_Jolt_NativeLibName Condition="'$(_NativePackages_Jolt_NativeRuntime)' == 'win-x64'">joltc.dll</_NativePackages_Jolt_NativeLibName>
    <_NativePackages_Jolt_NativeLibName Condition="'$(_NativePackages_Jolt_NativeRuntime)' == 'win-arm64'">joltc.dll</_NativePackages_Jolt_NativeLibName>
    <_NativePackages_Jolt_NativeLibName Condition="'$(_NativePackages_Jolt_NativeRuntime)' == 'osx-x64'">libjoltc.dylib</_NativePackages_Jolt_NativeLibName>
    <_NativePackages_Jolt_NativeLibName Condition="'$(_NativePackages_Jolt_NativeRuntime)' == 'linux-x64'">libjoltc.so</_NativePackages_Jolt_NativeLibName>
  </PropertyGroup>
  <ItemGroup>
    <Content Condition="'$(_NativePackages_Jolt_NativeRuntime)' != ''" Include="$(MSBuildThisFileDirectory)..\..\runtimes\$(_NativePackages_Jolt_NativeRuntime)\native\$(_NativePackages_Jolt_NativeLibName)">
      <Link>%(Filename)%(Extension)</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      <Visible>False</Visible>
    </Content>
  </ItemGroup>
</Project>
